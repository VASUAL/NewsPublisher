{"ast":null,"code":"import HeadOfHome from \"../components/HeadOfHome\";\nimport request from \"../utils/request\";\nimport moment from \"moment\";\nexport default {\n  name: \"FirstNews\",\n  components: {\n    HeadOfHome\n  },\n\n  data() {\n    return {\n      tableData: []\n    };\n  },\n\n  methods: {\n    //进入新闻详情页\n    intoNewsContent(id) {\n      this.$router.push({\n        path: '/newsContent',\n        query: {\n          newsId: id\n        }\n      });\n    },\n\n    //加载数据\n    loadData() {\n      request.post(\"/news/getFirstNews\").then(res => {\n        for (let i = 0; i < res.data.length; i++) {\n          res.data[i].date = moment(res.data[i].date).format(\"YYYY-MM-DD\");\n        }\n\n        this.tableData = res.data;\n      });\n    }\n\n  },\n\n  created() {\n    document.body.style.overflow = 'auto';\n    this.loadData();\n  }\n\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,UAAP,MAAuB,0BAAvB;AACA,OAAOC,OAAP,MAAoB,kBAApB;AACA,OAAOC,MAAP,MAAmB,QAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;EAEbC,UAAU,EAAE;IAACJ;EAAD,CAFC;;EAGbK,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE;IADN,CAAP;EAKD,CATY;;EAUbC,OAAO,EAAE;IACP;IACAC,eAAe,CAACC,EAAD,EAAK;MAClB,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,cADU;QAEhBC,KAAK,EAAE;UACLC,MAAM,EAAEL;QADH;MAFS,CAAlB;IAMD,CATM;;IAUP;IACAM,QAAQ,GAAG;MACTd,OAAO,CAACe,IAAR,CAAa,oBAAb,EAAmCC,IAAnC,CAAwCC,GAAE,IAAI;QAC5C,KAAK,IAAIC,CAAC,GAAC,CAAX,EAAaA,CAAC,GAACD,GAAG,CAACb,IAAJ,CAASe,MAAxB,EAA+BD,CAAC,EAAhC,EAAoC;UAClCD,GAAG,CAACb,IAAJ,CAASc,CAAT,EAAYE,IAAZ,GAAmBnB,MAAM,CAACgB,GAAG,CAACb,IAAJ,CAASc,CAAT,EAAYE,IAAb,CAAN,CAAyBC,MAAzB,CAAgC,YAAhC,CAAnB;QACF;;QACA,KAAKhB,SAAL,GAAiBY,GAAG,CAACb,IAArB;MACD,CALD;IAMF;;EAlBO,CAVI;;EA8BbkB,OAAO,GAAG;IACRC,QAAQ,CAACC,IAAT,CAAcC,KAAd,CAAoBC,QAApB,GAA+B,MAA/B;IACA,KAAKZ,QAAL;EACF;;AAjCa,CAAf","names":["HeadOfHome","request","moment","name","components","data","tableData","methods","intoNewsContent","id","$router","push","path","query","newsId","loadData","post","then","res","i","length","date","forma