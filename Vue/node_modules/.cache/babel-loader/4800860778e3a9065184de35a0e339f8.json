{"ast":null,"code":"import request from \"../../utils/request\";\nimport { ElMessage } from \"element-plus\";\nimport E from 'wangeditor';\nlet editor;\nexport default {\n  name: \"PublishNews\",\n\n  data() {\n    return {\n      ruleForm: {\n        headline: '',\n        author: '',\n        content: '',\n        date: '',\n        type: '',\n        click: 0,\n        comment: 0,\n        first: 0\n      },\n      rules: {\n        headline: [{\n          required: true,\n          message: '请输入标题',\n          trigger: 'blur'\n        }],\n        date: [{\n          type: 'date',\n          required: true,\n          message: '请选择日期',\n          trigger: 'change'\n        }],\n        type: [{\n          required: true,\n          message: '请选择板块',\n          trigger: 'change'\n        }]\n      },\n      lists: []\n    };\n  },\n\n  methods: {\n    //重置表单\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n      editor.txt.html(\"\");\n    },\n\n    //提交表单\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          //如果表单不空\n          if (editor.txt.html() === '' || editor.txt.html() === \"\") {\n            ElMessage({\n              type: 'warning',\n              message: '请输入内容'\n            });\n          } else {\n            this.ruleForm.content = editor.txt.html();\n            request.post(\"/news/publishNews\", this.ruleForm).then(res => {\n              ElMessage({\n                type: 'success',\n                message: '发布成功'\n              });\n              this.resetForm(formName);\n            });\n          }\n        } else {\n          //如果表单为空\n          return false;\n        }\n      });\n    },\n\n    //加载数据\n    load() {\n      let userEmail = sessionStorage.getItem(\"userEmail\");\n      let reg = new RegExp('\"', \"g\");\n      let newStr = userEmail.replace(reg, \"\"); //正则表达式，将字符串两端的双引号去掉\n\n      this.ruleForm.author = newStr; //加载下拉列表数据\n\n      request.post(\"/type/getAllType\").then(res => {\n        this.lists = res.data;\n      }); //加载富文本编辑器\n\n      this.$nextTick(() => {\n        editor = new E('#div1');\n        editor.config.zIndex = 500;\n        editor.create();\n      });\n    }\n\n  },\n\n  created() {\n    this.load();\n  }\n\n};","map":{"version":3,"mappings":"AAsCA,OAAOA,OAAP,MAAoB,qBAApB;AACA,SAAQC,SAAR,QAAwB,cAAxB;AACA,OAAOC,CAAP,MAAc,YAAd;AAEA,IAAIC,MAAJ;AACA,eAAe;EACbC,IAAI,EAAE,aADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EADF;QAERC,MAAM,EAAE,EAFA;QAGRC,OAAO,EAAE,EAHD;QAIRC,IAAI,EAAE,EAJE;QAKRC,IAAI,EAAE,EALE;QAMRC,KAAK,EAAE,CANC;QAORC,OAAO,EAAE,CAPD;QAQRC,KAAK,EAAE;MARC,CADL;MAWLC,KAAK,EAAE;QACLR,QAAQ,EAAC,CACP;UACES,QAAQ,EAAE,IADZ;UAEEC,OAAO,EAAE,OAFX;UAGEC,OAAO,EAAE;QAHX,CADO,CADJ;QAQLR,IAAI,EAAE,CACJ;UACEC,IAAI,EAAE,MADR;UAEEK,QAAQ,EAAE,IAFZ;UAGEC,OAAO,EAAE,OAHX;UAIEC,OAAO,EAAE;QAJX,CADI,CARD;QAgBLP,IAAI,EAAE,CACJ;UACEK,QAAQ,EAAE,IADZ;UAEEC,OAAO,EAAE,OAFX;UAGEC,OAAO,EAAE;QAHX,CADI;MAhBD,CAXF;MAmCLC,KAAK,EAAE;IAnCF,CAAP;EAuCD,CA1CY;;EA2CbC,OAAO,EAAE;IACP;IACAC,SAAS,CAACC,QAAD,EAAW;MAClB,KAAKC,KAAL,CAAWD,QAAX,EAAqBE,WAArB;MACArB,MAAM,CAACsB,GAAP,CAAWC,IAAX,CAAgB,EAAhB;IACD,CALM;;IAMP;IACAC,UAAU,CAACL,QAAD,EAAW;MACnB,KAAKC,KAAL,CAAWD,QAAX,EAAqBM,QAArB,CAA+BC,KAAD,IAAW;QACvC,IAAIA,KAAJ,EAAW;UAAK;UACd,IAAI1B,MAAM,CAACsB,GAAP,CAAWC,IAAX,OAAoB,EAApB,IAAwBvB,MAAM,CAACsB,GAAP,CAAWC,IAAX,OAAoB,EAAhD,EAAoD;YAClDzB,SAAS,CAAC;cACRU,IAAI,EAAE,SADE;cAERM,OAAO,EAAE;YAFD,CAAD,CAAT;UAID,CALD,MAKM;YACJ,KAAKX,QAAL,CAAcG,OAAd,GAAwBN,MAAM,CAACsB,GAAP,CAAWC,IAAX,EAAxB;YACA1B,OAAO,CAAC8B,IAAR,CAAa,mBAAb,EAAiC,KAAKxB,QAAtC,EAAgDyB,IAAhD,CAAqDC,GAAE,IAAI;cACzD/B,SAAS,CAAC;gBACRU,IAAI,EAAE,SADE;gBAERM,OAAO,EAAE;cAFD,CAAD,CAAT;cAIA,KAAKI,SAAL,CAAeC,QAAf;YACD,CAND;UAOF;QACF,CAhBA,MAgBO;UAAS;UACd,OAAO,KAAP;QACF;MACD,CApBD;IAqBD,CA7BM;;IA8BP;IACAW,IAAI,GAAG;MACL,IAAIC,SAAQ,GAAIC,cAAc,CAACC,OAAf,CAAuB,WAAvB,CAAhB;MACA,IAAIC,GAAE,GAAI,IAAIC,MAAJ,CAAW,GAAX,EAAgB,GAAhB,CAAV;MACA,IAAIC,MAAK,GAAIL,SAAS,CAACM,OAAV