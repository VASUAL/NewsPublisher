{"ast":null,"code":"import HeadOfHome from \"../components/HeadOfHome\";\nimport request from \"../utils/request\";\nexport default {\n  name: \"AllType\",\n  components: {\n    HeadOfHome\n  },\n\n  data() {\n    return {\n      tableData: []\n    };\n  },\n\n  methods: {\n    //加载板块数据\n    loadData() {\n      request.post(\"/type/getAllType\").then(res => {\n        this.tableData = res.data;\n      });\n    },\n\n    //跳转到具体板块\n    intoTypeContent(name) {\n      this.$router.push({\n        path: '/typeContent',\n        query: {\n          typeName: name\n        }\n      });\n    }\n\n  },\n\n  created() {\n    this.loadData();\n  }\n\n};","map":{"version":3,"mappings":"AAwBA,OAAOA,UAAP,MAAuB,0BAAvB;AACA,OAAOC,OAAP,MAAoB,kBAApB;AACA,eAAe;EACbC,IAAI,EAAE,SADO;EAEbC,UAAU,EAAE;IAACH;EAAD,CAFC;;EAGbI,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE;IADN,CAAP;EAMD,CAVY;;EAWbC,OAAO,EAAE;IACP;IACAC,QAAQ,GAAG;MACTN,OAAO,CAACO,IAAR,CAAa,kBAAb,EAAiCC,IAAjC,CAAsCC,GAAE,IAAI;QAC1C,KAAKL,SAAL,GAAiBK,GAAG,CAACN,IAArB;MACD,CAFD;IAGD,CANM;;IAOP;IACAO,eAAe,CAACT,IAAD,EAAO;MACpB,KAAKU,OAAL,CAAaC,IAAb,CAAkB;QAChBC,IAAI,EAAE,cADU;QAEhBC,KAAK,EAAE;UACLC,QAAQ,EAAEd;QADL;MAFS,CAAlB;IAMF;;EAfO,CAXI;;EA4Bbe,OAAO,GAAG;IACR,KAAKV,QAAL;EACF;;AA9Ba,CAAf","names":["HeadOfHome","request","name","components","data","tableData","methods","loadData","post","then","res","intoTypeContent","$router","push","path","query","typeName","created"],"sourceRoot":"","sources":["/Users/wangjin/Desktop/newspublisher/Vue/src/layout/AllType.vue"],"sourcesContent":["<template>\n  <!--头部-->\n  <HeadOfHome/>\n  <!--导航栏-->\n  <div class=\"divFirst\">\n    <router-link to=\"/\" class=\"link1\">首页</router-link>\n    <router-link to=\"/latestNews\" class=\"link2\">最新</router-link>\n    <router-link to=\"/hotNews\" class=\"link3\">热门</router-link>\n    <router-link to=\"/firstNews\" class=\"link4\">置顶</router-link>\n    <router-link to=\"/allType\" class=\"link5\">全部板块</router-link>\n  </div>\n  <!--中间部分-->\n  <div class=\"divSecond\">\n    <el-table :data=\"tableData\" class=\"el1\" :show-header=\"false\">\n      <el-table-column align=\"center\">\n        <template #default=\"scope\" class=\"tem1\">\n          <el-button type=\"text\" class=\"el2\" @click=\"intoTypeContent(scope.row.name)\">{{scope.row.name}}</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n  </div>\n</template>\n\n<script>\nimport HeadOfHome from \"../components/HeadOfHome\";\nimport request from \"../utils/request\";\nexport default {\n  name: \"AllType\",\n  components: {HeadOfHome},\n  data() {\n    return {\n      tableData: [\n\n      ],\n\n    }\n  },\n  methods: {\n    //加载板块数据\n    loadData() {\n      request.post(\"/type/getAllType\").then(res =>{\n        this.tableData = res.data\n      })\n    },\n    //跳转到具体板块\n    intoTypeContent(name) {\n      this.$router.push({\n        path: '/typeContent',\n        query: {\n          typeName: name\n        }\n      })\n    }\n  },\n  created() {\n    this.loadData()\n  }\n}\n</script>\n\n<style scoped>\n.divFirst{\n  position: absolute;width: 100%;height: 45px;margin-top: 50.5px;background: white;box-shadow: 0 0 3px 0 #f3f3f3;\n}\n.link1{\n  position: absolute;transform: translate(-50%,-50%);left: 20%;top: 50%;font-size: 17px;color:black;\n}\n.link2{\n  position: absolute;transform: translate(-50%,-50%);left: 27%;top: 50%;color: black;font-size: 17px;\n}\n.link3{\n  position: absolute;transform: translate(-50%,-50%);left: 34%;top: 50%;color: black;font-size: 17px;\n}\n.link4{\n  position: absolute;transform: translate(-50%,-50%);left: 41%;top: 50%;color: black;font-size: 17px;\n}\n.link5{\n  position: absolute;transform: translate(-50%,-50%);left: 50%;top: 50%;color:#409eff;font-size: 17px;\n}\n.divSecond{\n  background: white;width: 60%;height: 400px;position: absolute;left: 20%;margin-top: 120px;box-shadow: 0 0 3px 3px #f3f3f3;\n}\n.el1{\n  width: 100%;\n}\n.tem1{\n  width: 100%;\n}\n.el2{\n  color:black;font-size: 17px\n}\n</style>"]},"